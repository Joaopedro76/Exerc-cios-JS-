<script>
  var valor1,valor2,resp;
 valor1=parseInt(prompt(“Digite o valor1”));
 valor2=parseInt(prompt(“Digite o valor2”));
 resp= valor1+valor2
  document.write(“A soma de valor1+valor2 é: "+resp);
 resp= valor1-valor2
   document.write(“A subtração de valor1-valor2 é: ”+ resp)
  resp= valor1*valor2
   document.write(“A multiplicação de valor1*valor2 é: ”+resp)
   resp= valor1/valor2
   document.write(“A divisão de valor1/valor2 é: ”+ resp)
 </script>



